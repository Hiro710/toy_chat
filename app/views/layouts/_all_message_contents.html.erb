<div>
  <% @message_contents.each do |msg_content| %>
    <p>名前：<%= msg_content.name %></p>
    <p>性別：<%= msg_content.sex %></p>
    <p>気分：<%= msg_content.mood %></p>
    <p>好きなタイプ：<%= msg_content.person_type %></p>
    <p>【メッセージ】<%= msg_content.content %></p>
    <p>投稿日時：<%= msg_content.created_at %></p>
  <% end %>
</div>
